    "userProperties": [],
    "typeProperties": {
        "notebook": {
            "referenceName": "Notebook 1",
            "type": "NotebookReference"
        },
        "snapshot": true,
        "sparkPool": {
            "referenceName": "cameleon",
            "type": "BigDataPoolReference"
        }
    }
}